# Database Connection Configuration
# This file defines how dbt connects to your database

# INSTRUCTIONS:
# 1. Copy this file to profiles.yml
# 2. Replace placeholders with your actual Snowflake credentials
# 3. NEVER commit profiles.yml to Git (it's in .gitignore)

dbt_learning:
  target: dev
  outputs:
    dev:
      type: snowflake
      account: your_account    # e.g., abc12345.us-east-1
      user: your_username
      password: your_password
      role: ACCOUNTADMIN       # or your assigned role
      warehouse: COMPUTE_WH    # your warehouse name
      database: ANALYTICS      # your database name
      schema: dbt_dev         # your default schema
      threads: 4
      client_session_keep_alive: False

